<%= form_for @cspace do |f| %>
  
  <%= f.label "Address" %>:
  <%= f.text_field :address1 %><br/>
  
  <%= f.label "City" %>:
  <%= f.text_field :address2 %><br/>

  <%= f.label "Latitude" %>:
  <%= f.text_field :latitude %><br/>
  
  <%= f.label "Longitude" %>:
  <%= f.text_field :longitude %><br/>

  <%= f.label "Square Feet" %>:
  <%= f.text_field :address1 %><br/>
  
  <%= f.label "Phone" %>:
  <%= f.text_field :phone %><br/>

  <%= f.label "Space Type" %>:
  <%= f.text_field :space_type %><br/>
  
  <%= f.label "Max Occupants" %>:
  <%= f.text_field :maximum_occupants %><br/>
  
  <%= f.label "Lease Length" %>:
  <%= f.text_field :lease_length %><br/>
  
  <%= f.label "Food Beverage" %>:
  <%= f.text_field :food_beverage %><br/>

  <%= f.label "Alcohol" %>:
  <%= f.text_field :alcohol %><br/>
  
  <%= f.label "Music Noise" %>:
  <%= f.text_field :music_noise %><br/>

  <%= f.label "WIFI" %>:
  <%= f.text_field :wifi %><br/>
  
  <%= f.label "Parking" %>:
  <%= f.text_field :parking %><br/>

  <%= f.label "Handicap" %>:
  <%= f.text_field :handicap %><br/>
  
  <%= f.label "Furnished" %>:
  <%= f.text_field :furnished %><br/>

  <%= f.label "Bathroom" %>:
  <%= f.text_field :bathroom %><br/>
  
  <%= f.label "Kitchen" %>:
  <%= f.text_field :kitchen %><br/>

  <%= f.label "Photo URL" %>:
  <%= f.text_field :photo_url %><br/>
  
  <%= f.label "Photo Caption" %>:
  <%= f.text_field :photo_caption %><br/>

  <%= f.submit %>
<% end %>