<%= form_for @cspace do |f| %>
  
  <%= f.label "Address" %>:
  <%= f.text_field :address1 %><br/>
  
  <%= f.label "City" %>:
  <%= f.text_field :address2 %><br/>

  <%= f.label :Latitude %>:
  <%= f.text_field :latitude %><br/>
  
  <%= f.label :Longitude %>:
  <%= f.text_field :longitude %><br/>

  <%= f.label "Square Feet" %>:
  <%= f.text_field :address1 %><br/>
  
  <%= f.label :Phone %>:
  <%= f.text_field :phone %><br/>

  <%= f.label "Space Type" %>:
  <%= f.text_field :space_type %><br/>
  
  <%= f.label "Max Occupants" %>:
  <%= f.text_field :maximum_occupants %><br/>






  <%= f.submit %>
<% end %>